<p id="notice"><%= notice %></p>

<h1>Employees</h1>

<table>
  <thead>
    <tr>
      <th>Emp</th>
      <th>Emp name</th>
      <th>Emp birthdate</th>
      <th>Emp religion</th>
      <th>Emp orihire</th>
      <th>Emp hire</th>
      <th>Emp resign</th>
      <th>Emp maritaldate</th>
      <th>Emp compid</th>
      <th>Emp email</th>
      <th>Emp gender</th>
      <th>Emp marital</th>
      <th>Emp bank</th>
      <th>Emp accbank</th>
      <th>Emp accholder</th>
      <th>Emp npwp</th>
      <th>Emp ktp</th>
      <th>Emp bpjstku</th>
      <th>Emp bpjskes</th>
      <th>Job name</th>
      <th>Lev name</th>
      <th>Branch name</th>
      <th>Emp mobile</th>
      <th>Emp orgid1</th>
      <th>Emp orgid4</th>
      <th>Emp pp</th>
      <th>Emp linemgr</th>
      <th>Emp mgremail</th>
      <th>Emp status</th>
      <th>Emp famcard</th>
      <th>Emp nationality</th>
      <th>Salary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employees.each do |employee| %>
      <tr>
        <td><%= employee.emp_id %></td>
        <td><%= employee.emp_name %></td>
        <td><%= employee.emp_birthdate %></td>
        <td><%= employee.emp_religion %></td>
        <td><%= employee.emp_orihire %></td>
        <td><%= employee.emp_hire %></td>
        <td><%= employee.emp_resign %></td>
        <td><%= employee.emp_maritaldate %></td>
        <td><%= employee.emp_compid %></td>
        <td><%= employee.emp_email %></td>
        <td><%= employee.emp_gender %></td>
        <td><%= employee.emp_marital %></td>
        <td><%= employee.emp_bank %></td>
        <td><%= employee.emp_accbank %></td>
        <td><%= employee.emp_accholder %></td>
        <td><%= employee.emp_npwp %></td>
        <td><%= employee.emp_ktp %></td>
        <td><%= employee.emp_bpjstku %></td>
        <td><%= employee.emp_bpjskes %></td>
        <td><%= employee.job_name %></td>
        <td><%= employee.lev_name %></td>
        <td><%= employee.branch_name %></td>
        <td><%= employee.emp_mobile %></td>
        <td><%= employee.emp_orgid1 %></td>
        <td><%= employee.emp_orgid4 %></td>
        <td><%= employee.emp_pp %></td>
        <td><%= employee.emp_linemgr %></td>
        <td><%= employee.emp_mgremail %></td>
        <td><%= employee.emp_status %></td>
        <td><%= employee.emp_famcard %></td>
        <td><%= employee.emp_nationality %></td>
        <td><%= employee.salary %></td>
        <td><%= link_to 'Show', employee %></td>
        <td><%= link_to 'Edit', edit_employee_path(employee) %></td>
        <td><%= link_to 'Destroy', employee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% if current_user.role =='Admin' %>
  <%= link_to 'New Employee', new_employee_path %>
<% elsif current_user.role =='Service' %>
<% elsif current_user.role =='Expat' %>  
<% end %>


